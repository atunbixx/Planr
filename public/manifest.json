      "icons": [{ "src": "/icons/budget-icon.svg", "sizes": "96x96", "type": "image/svg+xml" }]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+wedding",
      "url": "/dashboard?action=%s"
